{
  "name": "json-beautifier",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://jsonviewer.poopcode.com",
  "dependencies": {
    "codemirror": "^5.54.0",
    "d3-state-visualizer": "^1.3.2",
    "gh-pages": "^2.2.0",
    "jsonlint": "git+https://github.com/josdejong/jsonlint.git#85a19d7",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-codemirror2": "^7.2.0",
    "react-dom": "^16.13.1",
    "react-json-tree": "^0.11.2",
    "react-scripts": "3.4.1",
    "standard": "^14.3.4",
    "styled-components": "^5.1.1"
  },
  "standard": {
    "env": [
      "jest",
      "jasmine"
    ],
